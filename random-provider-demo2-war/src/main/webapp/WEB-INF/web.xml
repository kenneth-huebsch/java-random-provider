<web-app xmlns="http://jakarta.ee/xml/ns/jakartaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://jakarta.ee/xml/ns/jakartaee
                             http://jakarta.ee/xml/ns/jakartaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>SecureRandomServlet2</servlet-name>
        <servlet-class>com.qrypt.randomprovider.demo2.SecureRandomServlet2</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SecureRandomServlet2</servlet-name>
        <url-pattern>/securerandom</url-pattern>
    </servlet-mapping>

</web-app>